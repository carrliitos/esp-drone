CONFIG_I2C0_PIN_SDA=11
CONFIG_I2C0_PIN_SCL=10
CONFIG_I2C1_PIN_SDA=40
CONFIG_I2C1_PIN_SCL=41
CONFIG_SPI_PIN_MISO=37
CONFIG_SPI_PIN_MOSI=35
CONFIG_SPI_PIN_CLK=36
CONFIG_SPI_PIN_CS0=34
CONFIG_MPU_PIN_INT=12
CONFIG_ADC1_PIN=2
CONFIG_EXT01_PIN=1
